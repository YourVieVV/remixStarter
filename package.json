{
  "name": "remix-template",
  "version": "1.0.0",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css -- --style=compressed",
    "build:remix": "remix build",
    "dev": "concurrently \"npm run sass\" \"npm run dev:remix\"",
    "dev:remix": "remix dev --no-restart -c \"node server.js\"",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "sass": "sass --watch app/:app/",
    "generate:css": "sass styles/:app/styles/",
    "typecheck": "tsc",
    "lint": "eslint '*/**/*.{js,ts,tsx}'",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "prepare": "husky install"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^1.18.1",
    "@remix-run/express": "^1.18.1",
    "@remix-run/node": "^1.18.1",
    "@remix-run/react": "^1.18.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.18.1",
    "@remix-run/eslint-config": "^1.18.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.4.1",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.18.0",
    "chokidar": "^3.5.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "sass": "1.49.9",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "lint-staged": {
    "*.+(js|mjs|ts|tsx|css|md|yml)": [
      "prettier --write"
    ]
  }
}
